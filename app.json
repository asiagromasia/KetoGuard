{
  "expo": {
    "name": "KetoGuard",
    "slug": "KetoGuard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ketoIcon.png",
    "splash": {
      "backgroundColor": "#ebdc34",
      "image": "./assets/ketoIcon.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSRemindersUsageDescription": "This app uses reminders about upcoming weight assessment"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ketoIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions":[
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
